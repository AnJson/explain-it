{
  "name": "Explain it...",
  "version": "0.1.0",
  "description": "Using gpt3 to explain selected text in a simple way.",
  "manifest_version": 3,
  "key": "",
  "action": {
    "default_popup": "index.html",
    "default_title": "Open Explain it..."
  },
  "permissions": ["storage", "identity", "tabs", "activeTab", "scripting"],
  "optional_permissions": [
    "<all_urls>"
  ],
  "host_permissions": [
    "<all_urls>"
   ],
  "icons": {
    "16": "expl-icon-16.png",
    "32": "expl-icon-32.png",
    "48": "expl-icon-48.png",
    "64": "expl-icon-64.png",
    "128": "expl-icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["./static/js/content.js"],
      "all_frames": false,
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "./static/js/background.js",
    "type": "module"
  },
  "start_url": ".",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff"
}
